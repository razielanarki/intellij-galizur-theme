        "MemoryIndicator" :
        {
            "usedBackground"      : "memUsed",
            "allocatedBackground" : "memAlloc"
        },
        "List" :
        {
            "background": "panelDim"
        },
        "Plugins" :
        {
            "background"               : "panelDim",
            "SearchField.background"   : "panelDark",
            "lightSelectionBackground" : "selectionAlt",
            "hoverBackground"          : "focusHover",
            "tagForeground"            : "text",
            "tagBackground"            : "focus",
            "eapTagBackground"         : "warningFocus",
