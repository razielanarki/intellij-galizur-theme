            {
                "currentBranchBackground" : "selectionAlt",
                "hoveredBackground"       : "focusHover"
            }
        },
        "WelcomeScreen" :
        {
            "SidePanel.background" : "panelAlt",
            "Projects" :
            {
                "selectionBackground" : "selectionAlt",
                "actions.background"  : "selectionAlt"
            }
        }
